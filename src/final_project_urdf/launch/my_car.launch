<launch>
    <param name="robot_description" command="$(find xacro)/xacro $(find final_project_urdf)/urdf/my_car.xacro" />
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <!-- <arg name="world_name" value="$(find final_project_urdf)/worlds/box_house.world" /> -->
        <arg name="world_name" value="$(find final_project_urdf)/worlds/my_world.world" />

    </include>

    <node pkg="gazebo_ros" type="spawn_model" name="model" args="-urdf -model mycar -param robot_description"  />
</launch>